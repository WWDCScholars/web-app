{
  "private": true,
  "type": "module",
  "name": "wwdcscholars-web",
  "description": "WWDCScholars.com Website",
  "version": "3.14.0-pre.13",
  "license": "MIT",
  "author": {
    "name": "Moritz Sternemann",
    "url": "https://github.com/moritzsternemann",
    "email": "opensource@moritzsternemann.de"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/WWDCScholars/web-app.git"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development nuxt-ts",
    "build:prod": "cross-env NODE_ENV=production nuxt-ts build",
    "build:stage": "cross-env NODE_ENV=staging nuxt-ts build",
    "start:prod": "cross-env NODE_ENV=production nuxt-ts start",
    "start:stage": "cross-env NODE_ENV=staging nuxt-ts start",
    "release:create": "standard-version",
    "release:create:pre": "standard-version --prerelease pre",
    "release:build:prod": "cross-env NODE_ENV=production LOCAL=1 nuxt-ts build",
    "release:build:stage": "cross-env NODE_ENV=production LOCAL=1 nuxt-ts build",
    "release:sentry:prod": "cross-env NODE_ENV=production ./.github/release_sentry.sh",
    "release:sentry:pre": "cross-env NODE_ENV=staging ./.github/release_sentry.sh",
    "release:publish": "git push --follow-tags",
    "release": "npm run release:create && npm run release:build:prod && npm run release:publish && npm run release:sentry:prod",
    "release:pre": "npm run release:create:pre && npm run release:build:stage && npm run release:publish && npm run release:sentry:pre"
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^2.0.0",
    "@nuxtjs/sentry": "^4.0.3",
    "@nuxtjs/style-resources": "^1.0.0",
    "@wwdcscholars/cloudkit": "^3.4.1",
    "autosize": "^4.0.2",
    "awesome-phonenumber": "^2.38.0",
    "dayjs": "^1.8.26",
    "lodash.get": "^4.4.2",
    "node-sass": "^4.14.1",
    "nuxt": "~2.14.0",
    "nuxt-property-decorator": "^2.7.2",
    "pica": "^5.3.0",
    "pug": "^2.0.4",
    "pug-plain-loader": "^1.0.0",
    "sass-loader": "^8.0.2",
    "swiper": "^5.3.8",
    "throttle-debounce": "^3.0.1",
    "uuid": "^3.4.0",
    "vee-validate": "^3.4.0",
    "vue-awesome-swiper": "^4.1.1",
    "vue-flatpickr-component": "^8.1.6",
    "vue-lazyload": "^1.3.3",
    "vue-plausible": "^1.1.1",
    "vuex-class": "^0.3.2"
  },
  "devDependencies": {
    "@nuxt/types": "^2.14.0",
    "@nuxt/typescript-build": "^2.0.3",
    "@sentry/cli": "^1.52.3",
    "@types/apple-mapkit-js-browser": "^5.50.0",
    "@types/dotenv": "^8.2.0",
    "@types/node": "^13.13.5",
    "@types/pica": "^5.1.1",
    "@types/throttle-debounce": "^2.1.0",
    "@types/uuid": "^8.3.0",
    "cross-env": "^7.0.2",
    "standard-version": "^9.0.0",
    "typescript": "^3.9.7"
  }
}
